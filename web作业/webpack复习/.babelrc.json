{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": "10",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins":[
    ["@babel/plugin-transform-runtime"],
    "@babel/plugin-syntax-class-properties",
    ["@babel/plugin-proposal-decorators", {"legacy": true}]
  ]
}
